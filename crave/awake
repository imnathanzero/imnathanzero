echo 'Initialize ROM and Device manifests'
rm -rf .repo/local_manifests &&
git clone https://github.com/imnathanzero/RMX1941_local_manifests.git -b yaap-13 .repo/local_manifests
repo init -u https://github.com/yaap/manifest.git -b thirteen

# Sync the repositories
echo "Sync begin"
repo sync -j8 --no-tags --no-clone-bundle --current-branch

. build/envsetup.sh

# Build the ROM
export BUILD_USERNAME=nathan
export BUILD_HOSTNAME=supeRuser
echo "BUILD STARTS NOW"
lunch yaap_RMX1941-userdebug
m yaap
